<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VolunteerIndicator__c</fullName>
    <description>Visual indicator of volunteer status</description>
    <externalId>false</externalId>
    <formula>IMAGE( 
CASE(GW_Volunteers__Volunteer_Status__c, 
&quot;Active&quot;, &quot;/img/msg_icons/confirm16.png&quot;,
&quot;Inactive&quot;, &quot;/img/icon/resourceAbsence16.png&quot;,
&quot;Onboarding&quot;, &quot;/img/icon/sossession16.png&quot;, 
&quot;Initial Inquiry&quot;, &quot;/img/icon/omni24.png&quot;,
&quot;Prospective&quot;, &quot;/img/icon/csquestions16.png&quot;,
&quot;/img/x.gif&quot;), 
&quot;Volunteer status&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Volunteer</label>
    <required>false</required>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
