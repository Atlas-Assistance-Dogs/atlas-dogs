        <name>varDoBEffective</name>
        <dataType>Date</dataType>
        <expression>AddMonths({!Date_of_Birth}, 13)</expression>
    </formulas>
    <formulas>
        <name>varDogQualify</name>
        <dataType>Boolean</dataType>
        <expression>if({!varDoBEffective}&lt;today(),True,False)</expression>
