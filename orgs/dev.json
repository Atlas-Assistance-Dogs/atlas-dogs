{
  "orgName": "atlas-dog - Dev Org",
  "edition": "Developer",
  "settings": {
    "accountSettings": {
      "enableRelateContactToMultipleAccounts": true
    },
    "lightningExperienceSettings": {
      "enableS1DesktopEnabled": true
    },
    "chatterSettings": {
      "enableChatter": true
    },
    "userManagementSettings": {
      "enableNewProfileUI": true
    },
    "securitySettings": {
      "enableAdminLoginAsAnyUser": true,
      "sessionSettings": {
        "forceRelogin": false
      }
    },
    "languageSettings": {
      "enableTranslationWorkbench": true
    },
    "emailAdministrationSettings": {
      "enableEnhancedEmailEnabled": true
    },
    "flowSettings": {
      "enableFlowInterviewSharingEnabled": true,
      "enableFlowPauseEnabled": true
    }
  },
  "features": ["DefaultWorkflowUser"]
}
